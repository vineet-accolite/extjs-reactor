Ext.define('{appName}.view.main.header.HeaderView', {
	extend: 'Ext.Toolbar',
	xtype: 'headerview',
	cls: 'headerview',
	items: [
		{ 
			xtype: 'container',
			cls: 'headerviewtext',
			bind: { html: '\u007Bheading}' }
		},
		'->',
		{
			xtype: 'button',
			ui: 'headerbutton-toolbar',
			reference: 'detailtoggle',
			handler: 'onHeaderViewDetailToggle',
			iconCls: 'x-fa fa-arrow-left'
		}
	]
});
